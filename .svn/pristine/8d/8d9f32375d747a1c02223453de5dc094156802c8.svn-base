	public long writeToStream(OutputStream out) throws IOException {
		long size = 0;
		InputStream ins = this.getInputStream();
		byte[] buf = new byte[1024];
		int len = -1;
		while((len=ins.read(buf))>=0){
			size+=len;
			out.write(buf, 0, len);
		}
		out.flush();
		return size;
	}

	
	
